*{margin: 0;padding: 0; list-style:none; font-family:Microsoft Yahei;}
html{height:100%;width:100%;}
body{ background-image:url(../images/bg.gif); background-size:100% 100%; font-size:16px;}
a:link,a:hover,a:visited,a:active{text-decoration: none;color: inherit;}
input{ outline:none}


